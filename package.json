{
  "name": "@xwtec/eslint-config-legacy",
  "version": "4.1.19",
  "description": "ESLint Configuration for xwtec legacy projects",
  "main": "index.js",
  "keywords": [
    "xwtec",
    "eslint",
    "config",
    "legacy"
  ],
  "files": [
    "index.js"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
    "build:rules": "node -r esm ./scripts/rules",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier **/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml} --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint **/*.{js,mjs,vue}",
    "lint:markdown": "markdownlint **/*.md --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint build format dist"
  },
  "license": "MIT",
  "dependencies": {
    "@xwtec/eslint-config": "6.1.1",
    "babel-eslint": "10.0.3",
    "eslint-config-prettier": "6.9.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": "xwtec/eslint-config-legacy",
  "devDependencies": {
    "@commitlint/cli": "8.3.4",
    "@fisker/commitlint-config": "1.1.4",
    "@fisker/husky-config": "1.1.5",
    "@fisker/lint-staged-config": "1.0.8",
    "@xwtec/eslint-config": "6.1.1",
    "@xwtec/prettier-config": "1.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.8.0",
    "esm": "3.2.25",
    "husky": "4.0.6",
    "lint-staged": "9.5.0",
    "markdownlint-cli": "0.21.0",
    "mem": "6.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "write-prettier-file": "1.2.4"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/xwtec/eslint-config-legacy/issues"
  },
  "homepage": "https://github.com/xwtec/eslint-config-legacy#readme",
  "author": {
    "name": "xwtec",
    "url": "https://github.com/xwtec"
  },
  "prettier": "@xwtec/prettier-config",
  "sideEffects": false
}
